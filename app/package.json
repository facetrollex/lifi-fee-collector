{
  "name": "lf-fee-collector-app",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "sh -c 'npm run dev:api & npm run dev:worker & wait'",
    "dev:api": "tsx watch api.ts",
    "dev:worker": "tsx watch worker.ts",
    "build": "tsc --project tsconfig.json",
    "start:api": "node dist/api.js",
    "start:worker": "node dist/worker.js"
  },
  "dependencies": {
    "@typegoose/typegoose": "latest",
    "dotenv": "latest",
    "ethers": "^5",
    "express": "latest",
    "lifi-contract-types": "git@github.com:lifinance/lifi-contract-types.git",
    "mongoose": "latest",
    "winston": "latest"
  },
  "devDependencies": {
    "@types/express": "latest",
    "@types/node": "latest",
    "tsx": "latest",
    "typescript": "latest"
  }
}
